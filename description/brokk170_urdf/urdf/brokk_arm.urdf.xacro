<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro" name="brokk">
  <!-- Transmission is used to link actuators to joints 
  <xacro:include filename="$(find brokk_arm_description)/urdf/brokk_arm.transmission.xacro" />
  <xacro:include filename="$(find brokk_arm_description)/urdf/brokk_arm.gazebo.xacro" />-->
  <xacro:property name="joint_damping" value="100"/>
  <xacro:property name="PI" value="3.1415926535897931"/>
  <xacro:property name="mass" value="1" /> <!-- arbitrary value for mass -->
  <xacro:property name="width" value="0.1" /> <!-- Square dimensions (widthxwidth) of beams -->
  <xacro:property name="height1" value="2" /> <!-- Link 1 -->
  <xacro:property name="height2" value="1" /> <!-- Link 2 -->
  <xacro:property name="height3" value="1" /> <!-- Link 3 -->

  <xacro:macro name="brokk_arm" params="prefix joint_limited
    brokk_base_lower_limit:=${0.152}     brokk_base_upper_limit:=${2.983}
    brokk_body1_lower_limit:=${0.2513}    brokk_body1_upper_limit:=${2.2617}
    brokk_body2_lower_limit:=${0.076}    brokk_body2_upper_limit:=${1.444}
    brokk_body3_lower_limit:=${0.096}    brokk_body3_upper_limit:=${1.824}
    brokk_body4_lower_limit:=${0.128}    brokk_body4_upper_limit:=${2.4367}
    safety_limits:=true safety_pos_margin:=0.15
    brokk_arm_vel_limit:=50.0
    k_velocity :=1.0
    description_pkg:='brokk_arm_description'">


    <link name= "${prefix}world">
<visual>
        <geometry>
          <mesh  filename="package://brokk_arm_description/meshes/legs_final2.dae" />
        </geometry>
        <material name="LightGrey">
          <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
      </visual>
      <collision>
	<origin xyz="0.05 0 0" rpy="0 0 0" />
        <geometry>
         <mesh scale="0.0011 0.001 0.001" filename="package://brokk_arm_description/meshes/legs.stl" />
        </geometry>
      </collision>

    </link>


    <link name="${prefix}brokk_base" >
      <visual>
      <origin
        xyz="0 0 0.0"
        rpy="0 0 0" />
        <geometry>
          <mesh  filename="package://brokk_arm_description/meshes/base_final2.dae" />
        </geometry>
        <material name="LightGrey">
          <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
      </visual>
      <collision>
	<origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh scale="0.001 0.001 0.001"  filename="package://brokk_arm_description/meshes/base.stl" />
        </geometry>
      </collision>
   <inertial>
          <origin xyz="0 0 0" rpy="0 0 0" />
          <mass value="2.34"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>

    </link>


    <joint name="${prefix}brokk_jnt0" type="revolute">
      <parent link="${prefix}world" />
      <child link = "${prefix}brokk_base" />
      <origin xyz="0 0.0 0.59" rpy="0.0 0.0 1.57" />
      <axis xyz="0 0 -1" />
      <limit effort="100.0" lower="${brokk_base_lower_limit}" upper="${brokk_base_upper_limit}" velocity="${brokk_arm_vel_limit}"/>
      <dynamics damping="${joint_damping}"/>
    </joint>

    <link name="${prefix}brokk_body1">
      <visual>
        <geometry>
          <mesh  filename="package://brokk_arm_description/meshes/body1.dae"  />
        </geometry>
        <material name="LightGrey">
          <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
      </visual>
      <collision>
	<origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh scale="0.001 0.001 0.001" filename="package://brokk_arm_description/meshes/body1.stl" />
        </geometry>
      </collision>
   <inertial>
                <origin xyz="0 0 0" rpy="0 0 0" />
          <mass value="2.34"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
    </link>

    <joint name="${prefix}brokk_jnt1" type="revolute">
      <parent link="${prefix}brokk_base" />
      <child link = "${prefix}brokk_body1" />
      <origin xyz="0.493 0 0.078" rpy="0.0 -0.806 0.0" />
      <axis xyz="0 1 0" />
      <limit effort="100.0" lower="${brokk_body1_lower_limit}" upper="${brokk_body1_upper_limit}" velocity="${brokk_arm_vel_limit}"/>
<dynamics damping="${joint_damping}"/>
    </joint>

    <link name="${prefix}brokk_body2">
      <visual>
        <geometry>
          <mesh filename="package://brokk_arm_description/meshes/body2.dae" />
        </geometry>
        <material name="LightGrey">
          <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
      </visual>
      <collision>
	<origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
           <mesh scale="0.001 0.001 0.001" filename="package://brokk_arm_description/meshes/body2.stl" />
        </geometry>
      </collision>
   <inertial>
                <origin xyz="0 0 0" rpy="0 0 0" />
          <mass value="2.34"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
    </link>

    <joint name="${prefix}brokk_jnt2" type="revolute">
      <parent link="${prefix}brokk_body1" />
      <child link = "${prefix}brokk_body2" />
      <origin xyz="0 0 0.87" rpy="0.0 0.78 0.0" />
      <axis xyz="0 -1 0" />
      <limit effort="100.0" lower="${brokk_body2_lower_limit }" upper="${brokk_body2_upper_limit }" velocity="0.5"/>
      <dynamics damping="${joint_damping}"/>
    </joint>

    <link name="${prefix}brokk_body3">
      <visual>
         <geometry>
          <mesh  filename="package://brokk_arm_description/meshes/body3.dae" />
        </geometry>
        <material name="LightGrey">
          <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
      </visual>
      <collision>
	<origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh scale="0.001 0.001 0.001" filename="package://brokk_arm_description/meshes/body3.stl" />
        </geometry>
      </collision>
   <inertial>
                <origin xyz="0 0 0" rpy="0 0 0" />
          <mass value="2.34"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
    </link>

    <joint name="${prefix}brokk_jnt3" type="revolute">
      <parent link="${prefix}brokk_body2" />
      <child link = "${prefix}brokk_body3" />
      <origin xyz="1.37 0.0 0.01" rpy="0.0 1.75 0.0" />
      <axis xyz="0 -1 0" />
      <limit effort="100.0" lower="${brokk_body3_lower_limit }" upper="${brokk_body3_upper_limit }" velocity="0.5"/>
      <dynamics damping="${joint_damping}"/>
    </joint>

    <link name="${prefix}brokk_body4">
      <visual>
        <geometry>
          <mesh  filename="package://brokk_arm_description/meshes/body4.dae"/>
        </geometry>
        <material name="LightGrey">
          <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
      </visual>
      <collision>
	<origin xyz=" 0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh scale="0.001 0.001 0.001" filename="package://brokk_arm_description/meshes/body4.stl" />
        </geometry>
      </collision>
   <inertial>
                <origin xyz="0 0 0" rpy="0 0 0" />
          <mass value="2.34"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
    </link>

    <joint name="${prefix}brokk_jnt4" type="revolute">
      <parent link="${prefix}brokk_body3" />
      <child link = "${prefix}brokk_body4" />
      <origin xyz="0.89 0 0.0" rpy="0.0 -1.19 0.0" />
      <axis xyz="0 1 0" />
      <limit effort="100.0" lower="${brokk_body4_lower_limit }" upper="${brokk_body4_upper_limit }" velocity="2"/>
     <dynamics damping="${joint_damping}"/>
    </joint>

    <link name="${prefix}brokk_tip">
      <visual>
        <geometry>
          <mesh scale="0.01 0.01 0.01" filename="package://brokk_arm_description/meshes/tip.STL" />
        </geometry>
        <material name="LightGrey">
          <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
      </visual>
      <collision>
	<origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh scale="0.01 0.01 0.01" filename="package://brokk_arm_description/meshes/tip.STL" />
        </geometry>
      </collision>
   <inertial>
                <origin xyz="0 0 0" rpy="0 0 0" />
          <mass value="2.34"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
    </link>

    <joint name="${prefix}brokk_jnt5" type="fixed">
      <parent link="${prefix}brokk_body4" />
      <child link = "${prefix}brokk_tip" />
      <origin xyz="1.25 0 0.08" rpy="0.0 0.0 0.0" />		
      <axis xyz="0 1 0" />
      <limit effort="0" lower="0" upper="0" velocity="0"/>
      <dynamics damping="${joint_damping}"/>
    </joint>



  </xacro:macro>
</robot>
